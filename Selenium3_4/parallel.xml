<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Suite">
<listeners>
<listener class-name="testng.testNGReport.Listeners"></listener>
<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
</listeners>
  <test thread-count="5" name="Firefox Test">
  <parameter name="Browser Name" value="Firefox"></parameter>
    <classes>
      <class name="testngFrameworkAdvance.handleParallelExecution.ParallelExecution"/>
    </classes>
  </test> <!-- Test -->
  
   <test thread-count="5" name="Chrome Test">
  <parameter name="Browser Name" value="Chrome"></parameter>
    <classes>
       <class name="testngFrameworkAdvance.handleParallelExecution.ParallelExecution"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
